<div style="padding:20px">
    {% if user != null %}
        <div>
            <img src="/logo/logo.png" alt="" width="50px" height="50px">
            <p>{{ compagny.name }}</p>
        </div>
        <a href="{{ path('command_create') }}">Créer une commande</a>
        <a href="{{ path('commands', {id_state: 1}) }}">Commandes non traitée</a>
        <a href="{{ path('commands', {id_state: 2}) }}">Commandes traitée</a>
        <a href="{{ path('commands', {id_state: 3}) }}">Commandes payée</a>
        <a href="{{ path('commands', {id_state: 4}) }}">Commandes en retard</a>
        <a href="{{ path('logout')}}">Logout</a>
    {% else %}
        <a href="{{ path('login')}}">Login</a>
    {% endif %}
</div>